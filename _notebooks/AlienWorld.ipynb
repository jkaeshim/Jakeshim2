    "          <span class=\"menu-icon\">\n",
    "            <svg viewBox=\"0 0 18 15\" width=\"18px\" height=\"15px\">\n",
    "              <path d=\"M18,1.484c0,0.82-0.665,1.484-1.484,1.484H1.484C0.665,2.969,0,2.304,0,1.484l0,0C0,0.665,0.665,0,1.484,0 h15.032C17.335,0,18,0.665,18,1.484L18,1.484z M18,7.516C18,8.335,17.335,9,16.516,9H1.484C0.665,9,0,8.335,0,7.516l0,0 c0-0.82,0.665-1.484,1.484-1.484h15.032C17.335,6.031,18,6.696,18,7.516L18,7.516z M18,13.516C18,14.335,17.335,15,16.516,15H1.484 C0.665,15,0,14.335,0,13.516l0,0c0-0.82,0.665-1.483,1.484-1.483h15.032C17.335,12.031,18,12.695,18,13.516L18,13.516z\"/>\n",
    "            </svg>\n",
    "          </span>\n",
    "        </label>\n",
    "\n",
    "        <div class=\"trigger\"><a class=\"page-link\" href=\"/alienWorld/blogs/\">Blogs</a><a class=\"page-link\" href=\"/alienWorld/compsci/\">Time Box</a><a class=\"page-link\" href=\"/alienWorld/search/\">Search</a><a class=\"page-link\" href=\"/alienWorld/tags/\">Tags</a><a class=\"page-link\" href=\"/alienWorld/about/\">About Us</a><a class=\"page-link\" href=\"/alienWorld/\">Alien World</a></div>\n",
    "      </nav></div>\n",
    "</header>\n",
    "<main class=\"page-content\" aria-label=\"Content\">\n",
    "      <div class=\"wrapper\">\n",
    "        <!-- Liquid code, run by Jekyll, used to define location of asset(s) -->\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "<style>\n",
    "    #controls {\n",
    "        position: relative;\n",
    "        z-index: 2; /* Ensure the controls are on top */\n",
    "    }\n",
    "</style>\n",
    "\n",
    "<!-- Prepare DOM elements -->\n",
    "<!-- Wrap both the dog canvas and controls in a container div -->\n",
    "<div id=\"canvasContainer\">\n",
    "    <div id=\"controls\"> <!-- Controls -->\n",
    "        <!-- Background controls -->\n",
    "        <button id=\"toggleCanvasEffect\">Invert</button>\n",
    "        <!-- Dog controls -->\n",
    "        <input type=\"radio\" name=\"animation\" id=\"idle\">\n",
    "        <label for=\"idle\">Idle</label>\n",
    "        <input type=\"radio\" name=\"animation\" id=\"barking\">\n",
    "        <label for=\"barking\">Barking</label>\n",
    "        <input type=\"radio\" name=\"animation\" id=\"walking\" checked>\n",
    "        <label for=\"walking\">Walking</label>\n",
    "    </div>\n",
    "</div>\n",
    "\n",
    "<script type=\"module\">\n",
    "    import GameEnv from '/alienWorld/assets/js/alienWorld/GameEnv.js';\n",
    "    import {initCollisionManager} from '/alienWorld/assets/js/alienWorld/CollisionManager.js';\n",
    "    import Background from '/alienWorld/assets/js/alienWorld/Background.js';\n",
    "    import { initDog } from '/alienWorld/assets/js/alienWorld/CharacterDog.js';\n",
    "    import {initMonkey} from '/alienWorld/assets/js/alienWorld/CharacterMonkey.js';\n",
    "\n",
    "    // Prepare Background Image\n",
    "    const backgroundImg = new Image();\n",
    "    backgroundImg.src = '/alienWorld/images/alien_planet.jpg';  // Jekyll/Liquid puts filename here\n",
    "\n",
    "    // Prepare HTML with Background Canvas\n",
    "    const backgroundCanvas = document.createElement(\"canvas\");\n",
    "    backgroundCanvas.id = \"background\";\n",
    "    document.querySelector(\"#canvasContainer\").appendChild(backgroundCanvas);\n",
    "\n",
    "    // Prepare Dog Image\n",
    "    const dogImg = new Image();\n",
    "    dogImg.src = '/alienWorld/images/dogSprite.png';\n",
    "\n",
    "    // Prepare HTML with Dog Canvas\n",
    "    const dogCanvas = document.createElement(\"canvas\");\n",
    "    dogCanvas.id = \"characters\";\n",
    "    document.querySelector(\"#canvasContainer\").appendChild(dogCanvas);\n",
    "\n",
    "\n",
    "    // Prepare Monkey Image\n",
    "    const monkeyImg = new Image();\n",
    "    monkeyImg.src = \"/alienWorld/images/monkeySprite.png\";\n",
    "\n",
    "    // Prepare HTML with Monkey Canvas\n",
    "    const monkeyCanvas = document.createElement(\"canvas\");\n",
    "    monkeyCanvas.id = \"characters\";\n",
    "    document.querySelector(\"#canvasContainer\").appendChild(monkeyCanvas);\n",
    "\n",
    "    // Setup Globals\n",
    "    GameEnv.gameSpeed = 2;\n",
    "    GameEnv.controls = document.getElementById(\"controls\");\n",
    "\n",
    "    backgroundImg.onload = function () {\n",
    "\n",
    "        // Background object(s)\n",
    "        const backgroundSpeedRatio = 0.2\n",
    "        var backgroundObj = new Background(backgroundCanvas, backgroundImg, backgroundSpeedRatio);\n",
    "\n",
    "        // Character object(s)\n",
    "        const dogSpeedRatio = 0.2\n",
    "        var dogObj = initDog(dogCanvas, dogImg, dogSpeedRatio);\n",
    "        const monkeySpeedRatio = 0.7\n",
    "        var monkeyObj = initMonkey(monkeyCanvas, monkeyImg, monkeySpeedRatio);\n",
    "\n",
    "        var characterArray = [];\n",
    "        characterArray.push(dogObj);\n",
    "        characterArray.push(monkeyObj);\n",
    "\n",
    "        var collisionManager = initCollisionManager(characterArray);\n",
    "\n",
    "        // Game loop\n",
    "        function gameLoop() {\n",
    "            backgroundObj.update();\n",
    "            backgroundObj.draw();\n",
    "\n",
    "            for (let character of characterArray){\n",
    "                character.update();\n",
    "                character.draw();\n",
    "            }\n",
    "\n",
    "            collisionManager.checkForCollisions();\n",
    "\n",
    "            requestAnimationFrame(gameLoop);  // cycle game, aka recursion\n",
    "        }\n",
    "\n",
    "        // Window resize event\n",
    "        function setSize() {\n",
    "            GameEnv.setGameEnv();  // Update GameEnv dimensions\n",
    "\n",
    "            // Call the sizing method on all game objects\n",
    "            backgroundObj.size();\n",
    "            for (let character of characterArray){\n",
    "                character.size();\n",
    "            }\n",
    "        }\n",
    "\n",
    "        // Listen for window resize events and trigger the handleResize function\n",
    "        window.addEventListener('resize', setSize);\n",
    "\n",
    "        // Start game\n",
    "        setSize();\n",
    "        gameLoop();\n",
    "    }\n",
    "\n",
    "    /* Toggle \"canvas filter property\" */\n",
    "    var isFilterEnabled = true;\n",
    "    const defaultFilter = getComputedStyle(document.documentElement).getPropertyValue('--default-canvas-filter');\n",
    "\n",
    "    toggleCanvasEffect.addEventListener(\"click\", function () {\n",
    "        if (isFilterEnabled) {\n",
    "            backgroundCanvas.style.filter = \"none\";  // remove filter\n",
    "            dogCanvas.style.filter = \"none\";\n",
    "            monkeyCanvas.style.filter = \"none\";\n",
    "        } else {\n",
    "            backgroundCanvas.style.filter = defaultFilter; // Apply the default filter value\n",
    "            dogCanvas.style.filter = defaultFilter;\n",
    "            monkeyCanvas.style.filter = defaultFilter;\n",
    "        }\n",
    "        isFilterEnabled = !isFilterEnabled;  // switch boolean value\n",
    "    });\n",
    "</script>\n",
    "\n",
    "\n",
    "      </div>\n",
    "    </main><footer class=\"site-footer h-card\">\n",
